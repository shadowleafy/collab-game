{
	"compileOnSave": true,
	"compilerOptions": {
		"alwaysStrict": true,
		"checkJs": true,
		"jsx": "react-native",
		"lib": ["DOM", "ESNext"],
		"moduleResolution": "nodenext",
		"outDir": "./root/js",
		"rootDir": "./src",
		"target": "ESNext"
	},
	"exclude": ["./root/**/*", "./node_modules/**/*"],
	"include": ["./src/**/*"]
}
